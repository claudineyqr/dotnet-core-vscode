{
    "workbench.colorTheme": "Dracula",
    "workbench.iconTheme": "vscode-icons",
    "editor.suggestSelection": "first",
    "editor.fontSize": 16,
    "editor.rulers": [
        150
    ],
    "vsintellicode.modify.editor.suggestSelection": "enabled",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            100
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },   
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "gitlens.advanced.telemetry.enabled": false,
    "dotnet-test-explorer.enableTelemetry": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/bin": true,
        "**/obj": true,
    },
    "csharpsortusings.sort.usings.order": "System Microsoft",
    "csharpsortusings.sort.usings.splitGroups": false,
    "html-css-class-completion.includeGlobPattern": "**/*.{css,html,cshtml}",
    "vssolution.showOutputChannel": true,
    "vssolution.trackActiveItem": true,
    "vssolution.showMode": "explorer",
    "path-intellisense.extensionOnImport": true,
    "vsintellicode.java.completionsEnabled": false,
    "vsintellicode.python.completionsEnabled": false,
    "vssolution.solutionExplorerIcons": "current-theme",
    "dotnet.enableTelemetry": false,
    "csharp.semanticHighlighting.enabled": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "html-css-class-completion.enableEmmetSupport": true,
    "bracket-pair-colorizer-2.colorMode": "Independent",
    
}
