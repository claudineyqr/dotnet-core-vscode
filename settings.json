{
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.rulers": [80, 120, 150],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.inlineSuggest.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.suggestSelection": "first",
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "Visual Studio 2019 Dark",
  "[dart]": {
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "dart.previewFlutterUiGuides": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/bin": true,
    "**/obj": true,
    "**/node_modules": true,
    "**/*.csproj.user": true,
    "**/.vs": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/bin": true,
    "**/obj": true,
    "**/.vs": true
  },
  "omnisharp.enableEditorConfigSupport": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.enableDecompilationSupport": true,
  "omnisharp.organizeImportsOnFormat": true,
  "omnisharp.useModernNet": true,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "explorer.autoReveal": false,
  "telemetry.telemetryLevel": "off",
  "dotnet-test-explorer.enableTelemetry": false,
  "redhat.telemetry.enabled": false,
  "code-runner.enableAppInsights": false,
  "nxConsole.enableTelemetry": false,
  "code-runner.showRunIconInEditorTitleMenu": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "sonarlint.disableTelemetry": true,
  "sonarlint.output.showVerboseLogs": true,
  "sonarlint.output.showAnalyzerLogs": true,
  "sonarlint.trace.server": "verbose",
  "json.maxItemsComputed": 5000000,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "cSpell.language": "en,pt,pt_BR",
  "security.workspace.trust.untrustedFiles": "open",
  "git.mergeEditor": true,
  "git.showPushSuccessNotification": true,
  "git.allowForcePush": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "kite.showWelcomeNotificationOnStartup": false,
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "errorLens.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.virtualRepositories.enabled": false,
  "gitlens.telemetry.enabled": false,
  "gitlens.graph.statusBar.enabled": false,
  "emmet.includeLanguages": {
    "aspnetcorerazor": "html"
  },
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": false,
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "csharpextensions.useFileScopedNamespace": true,
  "[aspnetcorerazor]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  }
}
