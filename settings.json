{
    "debug.openDebug": "openOnDebugBreak",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "sync.gist": "",
    "sync.quietSync": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.suggestSelection": "first",
    "editor.fontSize": 15,
    "editor.rulers": [
        80,
        120,
        150
    ],
    "[csharp]": {
        "editor.formatOnSave": true
    },
    "[css]": {
        "editor.formatOnSave": true
    },
    "[javascript]": {
        "editor.formatOnSave": true,
    },
    "[json]": {
        "editor.formatOnSave": true,
    },
    "git.autofetch": true,
    "git.showPushSuccessNotification": true,
    "git.smartCommitChanges": "all",
    "html-css-class-completion.enableEmmetSupport": true,
    "workbench.colorTheme": "Visual Studio 2019 Dark",
    "workbench.iconTheme": "vscode-icons",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/bin": true,
        "**/obj": true,
        "**/node_modules": true
    },
    "search.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/bin": true,
        "**/obj": true
    },
    "gitlens.currentLine.enabled": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.hovers.enabled": false,
    "git.enableSmartCommit": true,
    "code-runner.enableAppInsights": false,
    "code-runner.showRunCommandInExplorerContextMenu": false,
    "code-runner.showRunIconInEditorTitleMenu": false,
    "omnisharp.enableDecompilationSupport": true,
    "omnisharp.enableEditorConfigSupport": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "runTerminalCommand.commands": [
        {
            "command": "dotnet run",
            "auto": true,
            "group": ".NET Core"
        },
        {
            "command": "dotnet add page -n ",
            "group": ".NET Core"
        },
        {
            "command": "dotnet watch run",
            "auto": true,
            "preserve": true,
            "group": ".NET Core"
        },
        {
            "command": "dotnet add package {clipboard}",
            "auto": true,
            "group": ".NET Core"
        },
        {
            "command": "dotnet bundle watch",
            "auto": true,
            "preserve": true,
            "group": ".NET Core"
        },
        {
            "command": "dotnet publish -c Release -f netcoreapp2.2 -r win10-x64 --self-contained true",
            "auto": true,
            "preserve": true,
            "group": ".NET Core Deploy"
        },
        {
            "command": "dotnet publish -c Release -f net5.0 -r win10-x64 --self-contained true",
            "auto": true,
            "preserve": true,
            "group": ".NET Core Deploy"
        },
    ],
    "explorer.autoReveal": false,
    "csharpFormatUsings.splitGroups": false,
}
